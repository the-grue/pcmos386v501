obj = ULCLS.OBJ ULDSPDEC.OBJ ULGETKEY.OBJ ULIFKEY.OBJ ULSCRLDN.OBJ ULSETVPT.OBJ ULWATHLP.OBJ
obj2 = ULBEEP.OBJ ULDRWBOX.OBJ ULEATKEY.OBJ ULFSIZE.OBJ ULGETKST.OBJ ULISMOS.OBJ ULQUALT.OBJ
obj3 = ULCHAR2V.OBJ ULDRWHBR.OBJ ULGETCHO.OBJ ULGETSTR.OBJ ULMESSAG.OBJ ULSETATR.OBJ ULSTR2V.OBJ 
obj4 = ULCLRBOX.OBJ ULDRWVBR.OBJ ULFILBOX.OBJ ULGETCUR.OBJ ULGETXY.OBJ ULSETCUR.OBJ
obj5 = ULSTOCK.OBJ ULSCRLUP.OBJ ULANYFIL.OBJ ULFORMT.OBJ ULOPCLWN.OBJ ULTRDIRL.OBJ ULVIEW.OBJ ULEXPTAB.OBJ ULRDLBL.OBJ ULWALKTR.OBJ ULMKPTH.OBJ ULREMFIL.OBJ ULTRDIR.OBJ ULWRLBL.OBJ
lib = ulib.lib

CC = wcc
CFLAGS = -zq
AR = wlib

$(lib): $(obj) $(obj2) $(obj3) $(obj4) $(obj5)
	$(AR) $@ -+$(obj)
	$(AR) $@ -+$(obj2)
	$(AR) $@ -+$(obj3)
	$(AR) $@ -+$(obj4)
	$(AR) $@ -+$(obj5)

.c.obj:
	$(CC) -fo=$@ $(CFLAGS) $<

clean: .symbolic
	del *.obj
	del $(lib)
	del *.bak
	del *.err
